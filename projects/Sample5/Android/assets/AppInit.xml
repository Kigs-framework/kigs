<?xml version="1.0"?>
<Inst N="AppWindow" Type="Window">
    <Attr N="PositionX" V="-1"/>
	<Attr N="PositionY" V="-1"/>
	<Attr N="SizeX" V="1280"/>
	<Attr N="SizeY" V="800"/>
	<Attr N="FullScreen" V="false"/>
	<Attr N="Mouse Cursor" V="true"/>
	<Attr Type="string" N="BundleFileName" V="files.bundle" Dyn="true"/>
	
	<Inst N="theRenderingScreen" Type="RenderingScreen">
		<Attr N="SizeX" V="0">
			<Modifier Type="CoreItemOperatorModifier" V="#..->SizeX#"/>
		</Attr>
		<Attr N="SizeY" V="0">
			<Modifier Type="CoreItemOperatorModifier" V="#..->SizeY#"/>
		</Attr>
		<Attr N="DesignSizeX" V="1280"/>
		<Attr N="DesignSizeY" V="800"/>
		<Attr N="BitsPerZ" V="32"/>
		<Attr N="Brightness" V="0"/>
		<Attr N="BackDropColor" V="{0.0,0.0,0.0}"/>
		<Attr N="VSync" V="true"/>
	</Inst>
	
	<Inst N="ConfirmExitPopup" Type="UIPanel">
	  <Attr N="Anchor" V="{0.500000,0.500000}"/>
	  <Attr N="SizeY" V="800"/>
	  <Attr N="SizeX" V="1280"/>
	  <Attr N="Opacity" V="0.600000"/>
	  <Attr N="Dock" V="{0.500000,0.500000}"/>
	  <Attr N="Color" V="{0.200000,0.200000,0.200000,1.000000}"/>
	  <Attr N="Priority" V="0"/>
	  <Inst N="BGBulle" Type="UIPanel">
		<Attr N="Anchor" V="{0.500000,0.500000}"/>
		<Attr N="SizeY" V="240"/>
		<Attr N="SizeX" V="500"/>
		<Attr N="Priority" V="500"/>
		<Attr N="Dock" V="{0.500000,0.500000}"/>
		<Attr N="Color" V="{1.000000,1.000000,1.000000,1.000000}"/>
		
		<Inst N="TextBulle" Type="UIText">            
			<Attr N="Color" V="{0.0,0.0,0.0,1.0}"/>
			<Attr N="Anchor" V="{0.500000,0.500000}"/>
			<Attr N="Text" V="Quitter l'appli ?"/>
			<Attr N="Priority" V="1"/>
			<Attr N="FontSize" V="40"/>
			<Attr N="FontName" V="Calibri.ttf"/>
			<Attr N="Opacity" V="1.000000"/>
			<Attr N="Dock" V="{0.500000,0.300000}"/>
			<Attr N="MaxSize" V="240"/>
      <Attr N="Priority" V="501"/>
		</Inst>
		
		<Inst N="YesButton" Type="UIButtonText">
		  <Attr N="ClickUpAction" V="OKExitConfirmationPopup"/>
		  <Attr N="Anchor" V="{0.500000,0.500000}"/>
		  <Attr N="Parameter" V="true"/>
		  <Attr N="UpText" V="OUI"/>
		  <Attr N="UpColor" V="{0.0,0.0,0.0,1.0}"/>
		  <Attr N="DownColor" V="{0.686,0.541,0.902,1.0}"/>
		  <Attr N="OverColor" V="{0.435,0.0,0.0,1.0}"/>
		  <Attr N="FontSize" V="50"/>
		  <Attr N="FontName" V="Calibri.ttf"/>
		  <Attr N="Dock" V="{0.300000,0.70000}"/>
      <Attr N="Priority" V="501"/>
		</Inst>
		
		<Inst N="NoButton" Type="UIButtonText">
		  <Attr N="ClickUpAction" V="CancelExitConfirmationPopup"/>
		  <Attr N="Anchor" V="{0.500000,0.500000}"/>
		  <Attr N="Parameter" V="true"/>
		  <Attr N="UpText" V="NON"/>
		  <Attr N="UpColor" V="{0.0,0.0,0.0,1.0}"/>
		  <Attr N="DownColor" V="{0.686,0.541,0.902,1.0}"/>
		  <Attr N="OverColor" V="{0.435,0.0,0.0,1.0}"/>
		  <Attr N="FontSize" V="50"/>
		  <Attr N="FontName" V="Calibri.ttf"/>
		  <Attr N="Dock" V="{0.700000,0.70000}"/>
      <Attr N="Priority" V="501"/>
		</Inst>
	  </Inst>
	</Inst>
</Inst>
